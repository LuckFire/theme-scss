{
	"compilerOptions": {
		"noUnusedLocals": true,
		"allowUnusedLabels": false,
		"allowUnreachableCode": false,
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"rootDir": "./src",
		"baseUrl": ".",
		"outDir": "dist",
		"paths": {
			"#utils/*": ["src/utils/*"],
			"#types/*": ["src/types/*"]
		}
	}
}